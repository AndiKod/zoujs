{
  "source": {
    "include": ["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|public/|docs)_"
  },
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "plugins": ["plugins/markdown"],
  "recurseDepth": 10,
  "opts": {
    "recurse": true,
    "destination": "./docs/jsdoc/",
    "tutorials": "./docs/jsTuts/",
    "readme": "./docs/jsReadme.md"
  }
}